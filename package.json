{
  "name": "std-changelog",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.2",
  "description": "Generate changelog.",
  "author": "standard-robots",
  "license": "MIT",
  "homepage": "https://github.com/standard-robots/changelog#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/standard-robots/changelog.git"
  },
  "bugs": "https://github.com/standard-robots/changelog/issues",
  "keywords": [
    "github",
    "gitlab",
    "release",
    "releases",
    "conventional",
    "changelog",
    "log"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./cli.mjs",
  "files": [
    "*.mjs",
    "dist"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag",
    "start": "nr dev && node cli.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "c12": "^1.10.0",
    "cac": "^6.7.14",
    "changelogen": "0.5.5",
    "convert-gitmoji": "^0.1.5",
    "execa": "^8.0.1",
    "kolorist": "^1.8.0",
    "ofetch": "^1.3.4",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/semver": "^7.5.8",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.0"
  }
}
